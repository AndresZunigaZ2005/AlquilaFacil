<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="399.0" prefWidth="278.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.alquila.controllers.VentanaIniciarSesionController">
    <children>
        <Button fx:id="btnIniciarSesion" layoutX="96.0" layoutY="282.0" mnemonicParsing="false" onAction="#iniciarSesion" text="Iniciar Sesion" />
        <Label fx:id="lblIniciarSesion" layoutX="58.0" layoutY="134.0" prefHeight="50.0" prefWidth="162.0" text="Iniciar Sesion" textAlignment="CENTER">
            <font>
                <Font size="28.0" />
            </font>
        </Label>
        <PasswordField fx:id="txtContrasenia" layoutX="64.0" layoutY="224.0" promptText="Contraseña" />
        <TextField fx:id="txtUsuario" layoutX="64.0" layoutY="187.0" promptText="Usuario" />
        <Label fx:id="lblNoTienesCuenta" layoutX="87.0" layoutY="330.0" prefHeight="8.0" prefWidth="102.0" text="¿No tienes cuenta?" />
        <Button fx:id="btnRegistrate" layoutX="95.0" layoutY="348.0" mnemonicParsing="false" onAction="#registrarse" prefHeight="26.0" prefWidth="86.0" text="Registrate" />
        <ImageView fx:id="imagenLogo" fitHeight="109.0" fitWidth="198.0" layoutX="40.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true" />
    </children>
</AnchorPane>
