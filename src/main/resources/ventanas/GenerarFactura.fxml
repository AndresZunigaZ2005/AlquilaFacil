<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<?import java.net.URL?>
<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="400" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.alquila.controllers.GenerarFacturaController">
    <stylesheets>
        <URL value="@../styles.css" />
    </stylesheets>
    <children>
        <Label layoutX="38" layoutY="32" text="Cédula del Cliente:" />
        <Label layoutX="38" layoutY="75" text="Placa del Vehículo:" />
        <HBox layoutX="160" layoutY="71" />
        <Button layoutX="230.0" layoutY="133.0" onAction="#crearFactura" text="Crear Factura" styleClass="save-button"/>
        <Button fx:id="atrasButton" layoutX="38.0" layoutY="133.0" onAction="#atras" text="Atrás" styleClass="save-button" />
      <ComboBox fx:id="comboBoxVehiculos" layoutX="155.0" layoutY="71.0" prefWidth="150.0" />
      <TextField fx:id="cedulaClienteField" styleClass="text-fieldDecorator" layoutX="155.0" layoutY="28.0" />
    </children>
</AnchorPane>
